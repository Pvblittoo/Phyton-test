import time
import random

dado=random.randint(1,6)
meta=30
j1=0
j2=0
turno=1

while j1<=meta and j2<=meta:

    if turno %2==0:
        print("turno del jugador 1")
    
        j1=j1+dado
        print("el jugador uno saco",j1)
    else :
        print("turno del jugador 2")
        j2=j2+dado

        
turno=turno+1









