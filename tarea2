print("ingrese su edad")
Edad=int(input())
if Edad<18 :
    print("usted es menor de edad")
else:
    print("usted es mayor de edad")

print("ingrese la cantidad de notas")

n1=float(input("ingrese 1 numero"))
n2=float(input("ingrese otro numero"))
n3=float(input("ingrese otro numero"))
total=n1+n2+n3
promedio=total/3
if promedio>=4:
    print("Usted aprobo")
else:
    print("usted reprobo")
